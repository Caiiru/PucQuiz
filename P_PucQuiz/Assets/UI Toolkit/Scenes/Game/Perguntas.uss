.panel {
    background-color: rgba(251, 247, 239, 0);
    flex-grow: 1;
    align-items: center;
    justify-content: center;
}

.unity-button {
    background-color: rgb(230, 63, 102);
    -unity-background-scale-mode: scale-to-fit;
    font-size: 76px;
    color: rgb(255, 255, 255);
    transition-property: scale;
    transition-duration: 0.15s;
    transition-timing-function: ease-out;
    scale: 1 1;
    align-self: flex-start;
    margin-top: 30px;
    margin-right: 30px;
    margin-bottom: 30px;
    margin-left: 30px;
    -unity-font-definition: resource('Font/LexendDeca-VariableFont_wght');
    -unity-font-style: bold;
    white-space: normal;
    flex-direction: row;
    align-items: center;
    align-content: flex-start;
    -unity-text-align: upper-left;
    justify-content: flex-start;
}

.unity-button:hover {
    scale: 1.1 1.1;
}

.unity-button:active {
    background-color: rgb(0, 0, 0);
    transition-duration: 2s;
}

.unity-label {
    -unity-font-style: bold;
    -unity-font-definition: url("project://database/Assets/UI%20Toolkit/Fonts/LexendDeca-VariableFont_wght.ttf?fileID=12800000&guid=e30bd63670e9bca42927092983a72b4c&type=3#LexendDeca-VariableFont_wght");
    color: rgb(0, 0, 0);
}

.ScaleUpDefault {
    width: 100%;
    height: 69%;
    justify-content: center;
    align-self: center;
    align-content: center;
    align-items: center;
    flex-direction: row;
    flex-wrap: wrap;
    scale: 1 1;
    transition-property: scale;
    transition-timing-function: ease-out;
    transition-duration: 0.2s;
    transition-delay: 3s;
}

.ScaleUpStart {
    scale: 0 0;
}

.TimerDefault {
    flex-grow: 1;
    align-self: center;
    justify-content: center;
    align-content: flex-start;
    transition-property: scale, opacity;
    transition-duration: 0.7s, 0.7s;
    transition-timing-function: ease-in, ease-out;
    transition-delay: 3s, 3s;
}

.QuestionTextDefault {
    flex-grow: 1;
    transition-property: translate, scale;
    transition-duration: 1s, 0.5s;
    transition-delay: 2s, 0s;
    transition-timing-function: ease-in, ease-out;
}

.TimerStart {
    opacity: 0;
    scale: 0 0;
    transition-delay: 0.1s;
}

.QuestionTextStart {
    translate: 0 295px;
    scale: 0 0;
}
